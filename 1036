#include <iostream>

using namespace std;
int main(){
    int n;
    char c;
    cin>>n>>c;
    int y=n/2.0+0.5;
    for(int i=0;i<n;i++)
        cout<<c;
    cout<<endl;
    for(int i=0;i<y-2;i++){
        cout<<c;
        for(int k=0;k<n-2;k++)
            cout<<' ';
        cout<<c<<endl;
    }
    for(int i=0;i<n;i++)
        cout<<c;
    cout<<endl;
    return 0;
}
